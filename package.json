{
  "name": "pj-angular",
  "version": "0.7.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "start:prod": "ng serve --prod=true",
    "build": "ng build ejustice-lib && ng build pj-angular",
    "build:prod": "ng build ejustice-lib --prod && ng build pj-angular --prod",
    "build:lib": "ng build ejustice-lib",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && ng build ejustice-lib && npm run build:prod",
    "pack:prod": "npm run clean && ng build ejustice-lib --prod && ng build --prod --sourceMap=false && web-ext build -s 'dist/pj-angular/'",
    "pack:dev": "ng build && web-ext build -s 'dist/pj-angular/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/cdk": "~17.0.0",
    "@angular/common": "^17.0.3",
    "@angular/compiler": "^17.0.3",
    "@angular/core": "^17.0.3",
    "@angular/forms": "^17.0.3",
    "@angular/localize": "^17.0.3",
    "@angular/material": "17.0.0",
    "@angular/platform-browser": "^17.0.3",
    "@angular/platform-browser-dynamic": "^17.0.3",
    "@angular/router": "^17.0.3",
    "core-js": "^3.26.1",
    "file-saver-es": "^2.0.5",
    "rxjs": "~7.6.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.1.0",
    "@angular/cli": "^17.0.1",
    "@angular/compiler-cli": "^17.0.3",
    "@angular/language-service": "^17.0.3",
    "@types/chrome": "0.0.203",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.53.0",
    "gulp": "^4.0.2",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^17.0.1",
    "protractor": "~7.0.0",
    "puppeteer": "^19.4.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.3",
    "typescript": "^5.2.2",
    "web-ext": "^7.4.0"
  }
}