{
  "name": "pj-angular",
  "version": "0.7.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "start:prod": "ng serve --prod=true",
    "build": "ng build ejustice-lib && ng build pj-angular",
    "build:prod": "ng build ejustice-lib --prod && ng build pj-angular --prod",
    "build:lib": "ng build ejustice-lib",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && ng build ejustice-lib && npm run build:prod",
    "pack:prod": "npm run clean && ng build ejustice-lib --prod && ng build --prod --sourceMap=false && web-ext build -s 'dist/pj-angular/'",
    "pack:dev": "ng build && web-ext build -s 'dist/pj-angular/'",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.1",
    "@angular/cdk": "~13.0.1",
    "@angular/common": "^13.0.1",
    "@angular/compiler": "^13.0.1",
    "@angular/core": "^13.0.1",
    "@angular/forms": "^13.0.1",
    "@angular/localize": "^13.0.1",
    "@angular/material": "13.0.1",
    "@angular/platform-browser": "^13.0.1",
    "@angular/platform-browser-dynamic": "^13.0.1",
    "@angular/router": "^13.0.1",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular-devkit/build-angular": "^13.0.2",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^13.0.2",
    "@angular/compiler-cli": "^13.0.1",
    "@angular/language-service": "^13.0.1",
    "@types/chrome": "0.0.163",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^16.11.7",
    "codelyzer": "^6.0.1",
    "gulp": "^3.9.1",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.8",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.3",
    "protractor": "~7.0.0",
    "puppeteer": "^11.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~4.4.4",
    "web-ext": "^6.5.0"
  }
}
