{
  "name": "pj-angular",
  "version": "0.7.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "start:prod": "ng serve --prod=true",
    "build": "ng build ejustice-lib && ng build pj-angular",
    "build:prod": "ng build ejustice-lib --prod && ng build pj-angular --prod",
    "build:lib": "ng build ejustice-lib",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && ng build ejustice-lib && npm run build:prod",
    "pack:prod": "npm run clean && ng build ejustice-lib --prod && ng build --prod --sourceMap=false && web-ext build -s 'dist/pj-angular/'",
    "pack:dev": "ng build && web-ext build -s 'dist/pj-angular/'",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/material": "11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "^0.1102.15",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^11.2.15",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/chrome": "0.0.124",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.11.8",
    "codelyzer": "^6.0.1",
    "gulp": "^4.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "puppeteer": "^5.3.1",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3",
    "web-ext": "^5.2.0"
  }
}